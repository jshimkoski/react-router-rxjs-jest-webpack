{
  "presets": [
    ["es2015", {
      "es2015": {
        "loose": true,
        "modules": false
      }
    }],
    "react",
    "stage-0"
  ],
  "plugins": [
    ["resolver", {"resolveDirs": ["src"]}],
    "syntax-dynamic-import",
    "system-import-transformer"
  ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs", "dynamic-import-node"]
    }
  }
}