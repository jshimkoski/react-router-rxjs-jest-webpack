{
  "name": "smilejs",
  "version": "1.0.0",
  "description": "A barebones UI framework",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist/",
    "build": "npm run clean && NODE_ENV=production webpack --config webpack.config.prod.js --progress -p --colors",
    "dev": "npm run clean && ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.config.dev.js",
    "prod": "npm run build && NODE_ENV=production npm start"
  },
  "author": "Jason Shimkoski <jshimkoski@me.com> (http://jasonshimmy.com)",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.25.0",
    "happypack": "^2.2.1",
    "html-webpack-plugin": "^2.24.0",
    "node-sass": "^3.10.1",
    "react-hot-loader": "^3.0.0-beta.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "express": "^4.14.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-helmet": "^3.1.0",
    "react-router": "^3.0.0",
    "rxjs": "^5.0.0-rc.1",
    "superagent": "^2.3.0"
  }
}
