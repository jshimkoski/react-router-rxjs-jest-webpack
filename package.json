{
  "name": "smilejs",
  "version": "1.0.0",
  "description": "A barebones UI framework",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "test": "jest",
    "clean": "rm -rf dist/ && rm -rf .happypack/",
    "build": "npm run clean && NODE_ENV=production webpack --config webpack.config.prod.js --progress -p --colors",
    "dev": "npm run clean && ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.config.dev.js",
    "prod": "npm run build && NODE_ENV=production npm start"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "^.+\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "author": "Jason Shimkoski <jshimkoski@me.com> (http://jasonshimmy.com)",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-resolver": "^1.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.5.1",
    "happypack": "^2.2.1",
    "html-webpack-plugin": "^2.24.0",
    "jest": "^16.0.2",
    "node-sass": "^3.10.1",
    "react-addons-test-utils": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.4",
    "react-test-renderer": "^15.3.2",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "express": "^4.14.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-helmet": "^3.1.0",
    "react-router": "^3.0.0",
    "rxjs": "^5.0.0-rc.1",
    "superagent": "^2.3.0"
  }
}
